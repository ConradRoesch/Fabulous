<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="nuget" value="https://api.nuget.org/v3/index.json" />
        <!-- Rebuilt Xamarin.Forms for new net6.0-ios -->
        <add key="github" value="https://nuget.pkg.github.com/fabulousfx/index.json" />
        <!-- Nightly builds for Fabulous -->
        <add key="fabulous" value="https://nuget.pkg.github.com/TimLariviere/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="USERNAME" />
            <add key="ClearTextPassword" value="TOKEN" />
        </github>
        <fabulous>
            <add key="Username" value="USERNAME" />
            <add key="ClearTextPassword" value="TOKEN" />
        </fabulous>
    </packageSourceCredentials>
</configuration>