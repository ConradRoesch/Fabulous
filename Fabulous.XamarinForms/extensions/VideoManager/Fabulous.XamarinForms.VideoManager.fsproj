<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\..\SourceLink.props" />
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <NoWarn>NU1701;NU5104</NoWarn>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>Fabulous.XamarinForms.VideoManager</PackageId>
    <Description>Fabulous extension for VideoManager</Description>
    <PackageTags>VideoManager;Audio;Video;Player</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Plugin.MediaManager.Forms.fs" />
    <None Include="Plugin.MediaManager.Forms.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="FSharp.Core" />
    <PackageReference Include="Plugin.MediaManager.Forms" />
  </ItemGroup>
  <ItemGroup Condition=" '$(IsPacking)' != 'true' ">
    <ProjectReference Include="..\..\src\Fabulous.XamarinForms\Fabulous.XamarinForms.fsproj" />
  </ItemGroup>
  <ItemGroup Condition=" '$(IsPacking)' == 'true' ">
    <PackageReference Include="Fabulous.XamarinForms" />
  </ItemGroup>
  <Import Project="..\..\..\Packages.targets" />
</Project>
